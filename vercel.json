{
  "version": 2,
  "builds": [{ "src": "api/**/*.ts", "use": "@vercel/node" }],
  "functions": {
    "api/server.ts": {
      "includeFiles": "dist/infoAidTech/**"
    }
  },
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/dist/infoAidTech/browser/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/server.ts"
    }
  ]
}
