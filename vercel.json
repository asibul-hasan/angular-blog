{
  "version": 2,
  "public": true,
  "name": "info-aid-tech",
  "builds": [
    {
      "src": "dist/infoAidTech/browser/**",
      "use": "@vercel/static"
    },
    {
      "src": "dist/infoAidTech/server/server.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/dist/infoAidTech/browser/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "dist/infoAidTech/server/server.mjs"
    }
  ]
}