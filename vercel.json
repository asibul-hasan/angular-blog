{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/infoAidTech/**"] }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/dist/infoAidTech/browser/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "dist/infoAidTech/server/server.mjs"
    }
  ]
}
